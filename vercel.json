{
  "rewrites": [
    {
      "source": "/",
      "destination": "/index.htm"
    },
    {
      "source": "/contact",
      "destination": "/tools/contact.html"
    },
    {
      "source": "/summer",
      "destination": "/models/summer.html"
    },
    {
      "source": "/models",
      "destination": "/models/allmodels.html"
    }
  ],
  "routes": [
    {
      "handle": "filesystem"
    },
    {
      "src": "/(.*)",
      "status": 404,
      "dest": "/404.html"
    }
  ]
}
